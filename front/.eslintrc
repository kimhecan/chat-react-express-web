{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [ //eslint가 react에서 쓰이는 import export지원하게, hooks지원하게
        //"import",
        //"react-hooks"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "jsx-a11y/label-has-associated-control":0,
        "object-curly-newline":0,
        "linebreak-style":0
    }
}
